{
  "name": "@realtimeregister/api",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite build --mode dev --watch ",
    "build": "vite build --mode prod",
    "test": "vitest",
    "preview": "pnpm run build && yalc push",
    "preview:dev": "vite build --mode dev && yalc push",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/realtimeregister/realtimeregister-ts",
  "keywords": [
    "realtimeregister",
    "domains",
    "ssl"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "./dist/api.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/api.js",
      "require": "./dist/api.cjs"
    },
    "./exceptions": {
      "types": "./dist/Exceptions.d.ts",
      "import": "./dist/exceptions.js",
      "require": "./dist/exceptions.cjs"
    }
  },
  "pnpm": {
    "overrides": {
      "vite@>=6.0.0 <=6.3.5": ">=6.3.6"
    }
  },
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@semantic-release/github": "^11.0.5",
    "@semantic-release/npm": "^12.0.1",
    "@stylistic/eslint-plugin-ts": "^2.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^24.0.0",
    "@types/qs": "^6.14.0",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.11.1",
    "eslint-plugin-import": "^2.31.0",
    "semantic-release": "^24.1.3",
    "terser": "^5.34.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "yalc": "1.0.0-pre.53"
  },
  "dependencies": {
    "axios": "^1.12.0",
    "qs": "^6.14.0"
  }
}
